{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "headers": [
        {
            "source": "/pureguard-extension.zip",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/zip"
                },
                {
                    "key": "Content-Disposition",
                    "value": "attachment; filename=pureguard-extension.zip"
                }
            ]
        }
    ]
}